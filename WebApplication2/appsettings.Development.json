{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Azure": {
    "TTS": {
      "ApiKey": "your_azure_api_key",
      "Region": "your_azure_region"
    }
  },
  "Silero": {
    "TTS": {
      "ApiUrl": "https://api.silero.ai/voice",
      "ApiToken": "your_silero_api_token"
    }
  },
  "Google": {
    "TTS": {
      "ApiKey": "your_google_api_key",
      "ProjectId": "your_google_project_id"
    }
  },
  "Microsoft": {
    "TTS": {
      "OutputPath": "C:\\Temp\\TTS" 
    }
  },
  "DefaultTTS": "microsoft"
}